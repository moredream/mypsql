<script type="text/javascript">
    $(document).ready(function() {

        FancyForm.setup();
        Closeup.setup();

        $('#ReportModal input[type=radio]').change(function() {
            $('#ReportModal .Button').removeClass('disabled');
            if ($(this).val() == 'other') {
                $('#ReportModal .Hidden').fadeIn(500);
            } else {
                $('#ReportModal .Hidden').fadeOut(500);
            }
        });

        $('#ReportCommentModal input[type=radio]').change(function() {
            $('#ReportCommentModal .Button').removeClass('disabled');
            if ($(this).val() == 'other') {
                $('#ReportCommentModal .Hidden').fadeIn(500);
            } else {
                $('#ReportCommentModal .Hidden').fadeOut(500);
            }
        });

    });

</script>

